        <Label Content="Email&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Kodeord&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="emailTextbox" HorizontalAlignment="Left" Height="23" Margin="70,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordTextbox" HorizontalAlignment="Left" Margin="70,37,0,0" VerticalAlignment="Top" Height="23" Width="120"/>
        <Button x:Name="loginButton" Content="Log ind&#xD;&#xA;" HorizontalAlignment="Left" Margin="100,65,0,0" VerticalAlignment="Top" Width="90" Height="23" Click="loginButton_Click"/>
        <Button x:Name="addMemberButton" Content="Opret medlem" HorizontalAlignment="Left" Margin="100,93,0,0" VerticalAlignment="Top" Width="90" Height="23" Click="addMemberButton_Click"/>
    </Grid>
</Window>
